{
  "name": "inventory-management-system",
  "version": "1.0.0",
  "description": "Full-stack inventory management system for off-license shops",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "start": "concurrently \"npm run backend:start\" \"npm run frontend:start\"",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:start": "cd frontend && npm start",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "build": "cd frontend && npm run build",
    "setup": "npm run install:all && echo 'Setup complete! Run npm run dev to start both servers'",
    "seed": "cd backend && npm run seed"
  },
  "keywords": [
    "inventory",
    "management",
    "nodejs",
    "nextjs",
    "mongodb",
    "qr-code",
    "pos"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
